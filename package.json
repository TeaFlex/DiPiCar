{
  "name": "dipicar",
  "version": "0.1.0",
  "description": "DiPiCar is an API aimed at controlling a car built by the user. This supports video streaming between the server and the client (if the car is equipped with a camera module).",
  "main": "dist/main.js",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.9.2",
    "helmet": "^4.4.1",
    "nexe": "^4.0.0-beta.17",
    "pigpio": "^3.3.0",
    "pistreamer": "^1.1.2",
    "stormdb": "^0.4.1",
    "typescript": "^4.1.3",
    "winston": "^3.3.3",
    "ws": "^7.4.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.27",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/ws": "^7.4.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.5.0"
  },
  "scripts": {
    "start": "tsc && NODE_ENV=production sudo node ./dist/main.js",
    "test": "jest",
    "build": "rm -rf ./dist && tsc && node build.script.js",
    "dev": "tsc && NODE_ENV=development sudo node ./dist/main.js"
  },
